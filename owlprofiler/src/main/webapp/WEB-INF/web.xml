<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <display-name>OWL Profiler</display-name>
    <servlet>
        <servlet-name>owlprofiler</servlet-name>
        <servlet-class>oeg.owlprofiler2.OwlprofilerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>owlprofiler</servlet-name>
        <url-pattern>/owlprofiler</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>owlprofiler.jsp</welcome-file>
    </welcome-file-list>
</web-app>